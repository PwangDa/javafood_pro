<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- 공통 -->
		<typeAlias type="java.util.List" alias="list" />
		<typeAlias type="java.util.Map" alias="map" />

		<!-- 다영 -->
		<typeAlias type="com.java.food.dto.CommentDTO" alias="commentDTO" />
		<typeAlias type="com.java.food.dto.AlbumDTO" alias="albumDTO" />
		<typeAlias type="com.java.food.dto.SongPlusDTO" alias="songPlusDTO" />
		
		<!-- 귀범 -->
		<typeAlias type="com.java.food.dto.FamousChartDTO" alias="chartDTO"/>
		
		<!-- 범주 -->
		<typeAlias type="com.java.food.dto.PlayListDTO" alias="playListDTO" />
		
		<!-- 용준 -->
		<typeAlias type="com.java.food.dto.GenreDTO" alias="genreDTO" />
		
		<!-- 경용 -->
		<typeAlias type="com.java.food.dto.login_DTO" alias="loginDTO" />
	</typeAliases>
</configuration>

